{
	"name": "FL Syntax Highlighting",
	"scopeName": "source.fl",
	"fileTypes": [
		"fl"
	],
	"patterns": [
		{
			"match": "#.*$",
			"name": "comment.line.number-sign.fl"
		},
		{
			"begin": "/\\*",
			"end": "\\*/",
			"name": "comment.block.fl"
		},
		{
			"match": "\\b(import)\\b",
			"name": "keyword.control.import.fl"
		},
		{
			"match": "\\b(int|str|float|bool|dict|list)\\b|\\$",
			"name": "storage.type.fl"
		},
		{
			"match": "\\b(key)\\b",
			"name": "storage.modifier.fl"
		},
		{
			"match": "\\b(const|True|False|if|else|while|for|return)\\b",
			"name": "keyword.control.fl"
		},
		{
			"match": "[0-9]+",
			"name": "constant.numeric.fl"
		},
		{
			"match": "\\+|\\-|\\*|\\/|\\%|\\>|\\<|\\=",
			"name": "keyword.operator.arithmetic.fl"
		},
		{
			"match": "[\\[\\]{}():,]",
			"name": "punctuation.definition.fl"
		}
	]
}
