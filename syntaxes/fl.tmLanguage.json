{
    "scopeName": "source.fl",
    "patterns": [
        {
            "match": "#.*$",
            "name": "comment.line.number-sign.fl"
        },
        {
            "match": "//.*$",
            "name": "comment.line.double-slash.fl"
        },
        {
            "match": "\\b(const)\\b",
            "name": "keyword.other.const.fl"
        },
        {
            "match": "\\b(float|str|int|bool|list)\\b",
            "name": "storage.type.fl"
        },
        {
            "match": "\\b(True|False)\\b",
            "name": "constant.language.boolean.fl"
        },
        {
            "match": "\\b(PI)\\b",
            "name": "constant.language.float.fl"
        },
        {
            "match": "\\b\\d+\\b",
            "name": "constant.numeric.integer.fl"
        },
        {
            "match": "\\b\\d+\\.\\d+\\b",
            "name": "constant.numeric.float.fl"
        },
        {
            "match": "\".*?\"",
            "name": "string.quoted.double.fl"
        }
    ],
    "repository": {},
    "names": "FL",
    "fileTypes": ["fl", "fls"]
}
